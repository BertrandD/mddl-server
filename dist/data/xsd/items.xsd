<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="item" minOccurs="1" maxOccurs="unbounded">
					<xs:complexType>
                        <xs:sequence>
                            <xs:choice maxOccurs="unbounded" minOccurs="0">
                                <xs:element name="stats" maxOccurs="1">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="set" minOccurs="1" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:attribute name="stat" use="required">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="SLOT_CARGO" />
                                                                <xs:enumeration value="SLOT_ENGINE" />
                                                                <xs:enumeration value="SLOT_MODULE" />
                                                                <xs:enumeration value="SLOT_TECHNOLOGY" />
                                                                <xs:enumeration value="SLOT_WEAPON" />
                                                                <xs:enumeration value="RESOURCE_FEO" />
                                                                <xs:enumeration value="RESOURCE_C" />
                                                                <xs:enumeration value="RESOURCE_CH4" />
                                                                <xs:enumeration value="RESOURCE_ATO3" />
                                                                <xs:enumeration value="RESOURCE_H2O" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:attribute>
                                                    <xs:attribute name="op" use="required">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="DIFF" />
                                                                <xs:enumeration value="PER" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:attribute>
                                                    <xs:attribute name="value" type="xs:double" use="required" />
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="requirements" minOccurs="0" maxOccurs="1">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="item" minOccurs="0" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:attribute name="id" type="xs:normalizedString" use="required" />
                                                    <xs:attribute name="count" type="xs:long" use="required" />
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="building" minOccurs="0" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:attribute name="id" type="xs:normalizedString" use="required" />
                                                    <xs:attribute name="level" type="xs:integer" use="required" />
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="properties" minOccurs="0" maxOccurs="1">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="set" minOccurs="1" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:attribute name="name" use="required">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="shield_defense" />
                                                                <xs:enumeration value="affected" />
                                                                <xs:enumeration value="multiplicator" />
                                                                <xs:enumeration value="power" />
                                                                <xs:enumeration value="capacity" />
                                                                <xs:enumeration value="slot_cargo" />
                                                                <xs:enumeration value="slot_engine" />
                                                                <xs:enumeration value="slot_module" />
                                                                <xs:enumeration value="slot_technology" />
                                                                <xs:enumeration value="slot_weapon" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:attribute>
                                                    <xs:attribute name="value" type="xs:string" use="required" />
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:choice>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:string" use="required" />
                        <xs:attribute name="nameId" type="xs:string" use="required" />
                        <xs:attribute name="type" type="xs:normalizedString" use="required" />
                        <xs:attribute name="descriptionId" type="xs:string" use="required" />
                        <xs:attribute name="rank" use="optional">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="NONE" />
                                    <xs:enumeration value="A" />
                                    <xs:enumeration value="B" />
                                    <xs:enumeration value="C" />
                                    <xs:enumeration value="D" />
                                    <xs:enumeration value="E" />
                                    <xs:enumeration value="F" />
                                    <xs:enumeration value="G" />
                                    <xs:enumeration value="H" />
                                    <xs:enumeration value="I" />
                                    <xs:enumeration value="J" />
                                    <xs:enumeration value="K" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="buildTime" type="xs:long" use="optional" />
                        <xs:attribute name="weight" type="xs:long" use="optional" />
                        <xs:attribute name="volume" type="xs:long" use="optional" />
                        <xs:attribute name="rarity" type="xs:integer" use="optional" />
                    </xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>