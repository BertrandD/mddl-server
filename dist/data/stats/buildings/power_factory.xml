<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../xsd/buildings.xsd">
    <building id="power_factory" nameId="building_power_factory_n" type="PowerFactory" descriptionId="building_power_factory_d" maxLevel="1">
        <buildTime>
            <set fromLevel="1" function="5*$level" />
        </buildTime>
        <properties>
            <property name="production" value="(200 + (100 * $level))"/>
        </properties>
        <requirements>
            <requirement level="1">
                <item id="resource_feo" count="15" />
                <item id="resource_ch4" count="10" />
            </requirement>
        </requirements>
    </building>
</list>