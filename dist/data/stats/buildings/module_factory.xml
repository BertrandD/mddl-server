<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../xsd/buildings.xsd">
    <building id="module_factory" type="ModuleFactory" nameId="module_factory_n" maxLevel="8" descriptionId="module_factory_d" buildTimeFunc="10*$level">
        <propertiesByLevel>
            <properties level="1">
                <list name="unlock_modules">
                    <set name="module" value="module_optimizer_feo" />
                    <set name="module" value="module_optimizer_c" />
                </list>
            </properties>
            <properties level="2">
                <list name="unlock_modules">
                    <set name="module" value="module_optimizer_ato3" />
                </list>
            </properties>
        </propertiesByLevel>
        <requirements>
            <requirement level="1">
                <function type="ITEM" id="resource_feo" val="100*(Math.pow($level,2))" />
            </requirement>
            <requirement level="2">
                <function type="ITEM" id="resource_feo" val="100*(Math.pow($level,2))" />
            </requirement>
            <requirement level="3">
                <function type="ITEM" id="resource_steel" val="100*(Math.pow($level,2))" />
                <building id="mine_metal" level="1" />
            </requirement>
            <requirement level="4">
                <function type="ITEM" id="resource_steel" val="100*(Math.pow($level,2))" />
            </requirement>
            <requirement level="5">
                <function type="ITEM" id="resource_steel" val="100*(Math.pow($level,2))" />
                <building id="mine" level="2" />
                <building id="storage" level="2" />
            </requirement>
            <requirement level="6">
                <function type="ITEM" id="resource_steel" val="100*(Math.pow($level,2))" />
            </requirement>
            <requirement level="7">
                <function type="ITEM" id="resource_steel" val="100*(Math.pow($level,2))" />
            </requirement>
            <requirement level="8">
                <function type="ITEM" id="resource_steel" val="100*(Math.pow($level,2))" />
            </requirement>
        </requirements>
    </building>
</list>